{
  "incremental": true,
  "indentWidth": 2,
  "yapf": {
    "indentWidth": 4,
    "based_on_style": "pep8"
  },
  "includes": ["**/*.{py,json,js,ts,md}"],
  "excludes": [
    "executable/target"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.62.1.wasm",
    "https://plugins.dprint.dev/json-0.7.2.wasm",
    "https://plugins.dprint.dev/markdown-0.12.1.wasm",
    "https://plugins.dprint.dev/yapf-0.2.1.exe-plugin@5f9e484ae7ee913ff7a9bf37e956fc13f0126732e272bbf64cf91936b05a4cee"
  ]
}
