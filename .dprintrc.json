{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "projectType": "openSource",
  "incremental": true,
  "yapf": {
    "based_on_style": "pep8"
  },
  "includes": ["**/*.{py,json,js,ts,md}"],
  "excludes": [
    "executable/target"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.33.0.wasm",
    "https://plugins.dprint.dev/json-0.7.2.wasm",
    "https://plugins.dprint.dev/markdown-0.4.2.wasm",
    "https://plugins.dprint.dev/yapf-0.2.0.exe-plugin@14c42b703709e81f813c6674a8110c522af0ea78b6298f4f73721121a1a03701"
  ]
}
