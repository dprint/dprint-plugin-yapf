{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "projectType": "openSource",
  "incremental": true,
  "yapf": {
    "based_on_style": "pep8"
  },
  "includes": ["**/*.{py,json,js,ts,md}"],
  "excludes": [
    "executable/target"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.29.0.wasm",
    "https://plugins.dprint.dev/json-0.7.0.wasm",
    "https://plugins.dprint.dev/markdown-0.4.0.wasm",
    "https://plugins.dprint.dev/yapf-0.1.0.exe-plugin@ec45d2523526ae6c350a7320cf3c76b78474d358e3e58ace008774aee31d45e1"
  ]
}
